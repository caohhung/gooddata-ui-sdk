{
    "_note": "This file is only for IDE. For build use tsconfig.[dev/build].json (dev mode is less strict)",
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "lib": ["es7", "dom"],
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "importHelpers": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "dist",
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "baseUrl": "..",
        "types": ["cypress-real-events", "node", "@cypress/grep"]
    },
    "compileOnSave": false,
    "exclude": ["**/node_modules", "dist", "esm"]
}
